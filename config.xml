<?xml version='1.0' encoding='utf-8'?>
<widget id="com.tealduck.webapp" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Teal Duck</name>
    <description>
        Teal Duck WebApp. First run without debug.
    </description>
    <author email="nssreejith229@gmail.com" href="http://cordova.io">
        Sreejith N
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
  <icon src="res/icon.png" width="180" height="180" />
        <allow-intent href="market:*" />
    </platform>
	<icon src="res/icon.png" />
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-statusbar" spec="^2.2.3" />
	<preference name="StatusBarOverlaysWebView" value="false" />
</widget>
